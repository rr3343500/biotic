        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.BioticClasses"
        android:usesCleartextTraffic="true"
        tools:ignore="Instantiatable">
        <activity android:name=".Activity.ViewTestActivity"></activity>
        <activity android:name=".Activity.LanguageActivity" />
        <activity android:name=".Activity.ScoreActivity" />
        <activity android:name=".Activity.ProfileActivity" />
        <activity android:name=".Activity.MyTestsActivity" />
        <activity android:name=".Activity.TestListActivity" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.TakeTestActivity"
            />
        <activity android:name=".Activity.ForgetPassword" />
        <activity android:name=".Activity.SignUpActivity" />
        <activity android:name=".Activity.LoginActivity">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>
    </application>

</manifest>